{
  "name": "wikidata-lang",
  "version": "2.0.2",
  "description": "an index of 183 languages accessible either by their 2-letters languages code or Wikidata Qid",
  "main": "src/index.js",
  "scripts": {
    "test": "mocha --compilers coffee:coffee-script/register",
    "build": "coffee -c src/index.coffee && browserify src/index.js -s wdLang > dist/index.js",
    "prepublish": "npm test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxlath/wikidata-lang.git"
  },
  "keywords": [
    "lang",
    "wikidata"
  ],
  "author": "maxlath",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maxlath/wikidata-lang/issues"
  },
  "homepage": "https://github.com/maxlath/wikidata-lang#readme",
  "devDependencies": {
    "browserify": "^13.0.0",
    "coffee-script": "^1.10.0",
    "mocha": "^2.3.4",
    "should": "^8.1.0"
  }
}
